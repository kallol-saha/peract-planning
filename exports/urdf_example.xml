<?xml version="1.0"?>
<robot name="crackers_example">
  
  <!-- Example URDF link using the exported crackers_visual -->
  <link name="crackers_link">
    
    <!-- Visual element using the exported mesh and material properties -->
    <visual>
      <geometry>
        <mesh filename="package://your_package/meshes/crackers_visual.obj" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="crackers_material">
        <!-- Color from exported data: RGB(1.000, 1.000, 1.000) with alpha 0.500 -->
        <color rgba="1.0 1.0 1.0 0.5"/>
        <!-- If you have a texture file, you can reference it here -->
        <!-- <texture filename="package://your_package/textures/crackers_texture.png"/> -->
      </material>
    </visual>
    
    <!-- Collision element (you might want to use the simpler 'crackers' object for collision) -->
    <collision>
      <geometry>
        <mesh filename="package://your_package/meshes/crackers.obj" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
    
    <!-- Inertial properties (you'll need to calculate these based on your needs) -->
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    
  </link>
  
</robot>
